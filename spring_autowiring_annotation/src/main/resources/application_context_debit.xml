<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
          http://www.springframework.org/schema/beans/spring-beans.xsd
          http://www.springframework.org/schema/context
          http://www.springframework.org/schema/context/spring-context.xsd">
    
    <context:annotation-config/>      
  
  <bean id="dcard" class="di.DebitCard"/>
   
 
 <bean id="add1" class="di.Address">
   <constructor-arg type = "String" value = "Snapdeal"/>
   <constructor-arg type = "String" value = "Bangalore"/>
 </bean>
 
 <bean id="add2" class="di.Address">
   <constructor-arg type = "String" value = "Snapdeal_2"/>
   <constructor-arg type = "String" value = "Bangalore_2"/>
 </bean>
 
 
  </beans>        